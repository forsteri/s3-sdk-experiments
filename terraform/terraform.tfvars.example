# AWS設定
aws_region  = "ap-northeast-1"
aws_profile = "your-sso-profile-name"
environment = "dev"

# S3設定
experiment_bucket_name = "your-unique-bucket-name-s3-experiments"
enable_versioning     = true
enable_monitoring     = false

# 追加タグ
additional_tags = {
  Owner       = "your-name"
  CostCenter  = "experiments"
  DeleteAfter = "2024-12-31"
}